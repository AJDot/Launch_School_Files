<h4>Email</h4>
<p><%= @email %></p>

<h4>Interests</h4>
<p><%= @interests.join(", ") %></p>

<h4>Other Users</h4>
<ul>
  <%@users.each do |name, _|%>
    <% unless name == @name %>
      <li>
        <a href="/users/<%= name %>">
          <%= name %>
        </a>
      </li>
    <% end %>
  <% end %>
</ul>
